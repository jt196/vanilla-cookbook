		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.18.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"eslint": "^9.21.0",
