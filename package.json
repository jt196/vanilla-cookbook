{
	"name": "sk-prisma-quickstart",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm prisma generate && vite build && prisma migrate deploy && pnpm prisma db seed",
		"serve": "node build/index.js",
		"generate-sw": "node src/lib/utils/pwa/generate-sw.js && ./sw-domain.sh",
		"test": "vitest",
		"test:watch": "pnpm test -- --watch",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"start": "pnpm build && pnpm generate-sw && pnpm serve"
	},
